{
  "name": "skeleton",
  "version": "2.0.0",
  "description": "Skeleton is a model view controller(mvc) pattern built on slim3.",
  "scripts": {
    "dev": "node node_modules/.bin/sponge scripts && node node_modules/.bin/sponge sass",
    "watch": "node node_modules/.bin/sponge watch",
    "build": "node node_modules/.bin/sponge build",
    "unbuild": "node node_modules/.bin/sponge unbuild",
    "pretest": "phplint app/**/*.php && phplint bootstrap/app.php && phplint config/*.php && phplint routes/*.php && phplint system/*.php",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigoiii/skeleton.git"
  },
  "keywords": [
    "micro framework",
    "skeleton",
    "slim",
    "php",
    "webpack",
    "gulp",
    "gulp-autoprefixer",
    "gulp-cache",
    "gulp-csscomb",
    "gulp-cssnano",
    "gulp-flatten",
    "gulp-if",
    "gulp-load-plugins",
    "gulp-merge-media-queries",
    "gulp-plumber",
    "gulp-uglify",
    "gulp-useref",
    "tools",
    "optimize"
  ],
  "author": "rodrigogalura3rd@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rodrigoiii/skeleton/issues"
  },
  "homepage": "https://github.com/rodrigoiii/skeleton#readme",
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "fs-extra": "^7.0.0",
    "mocha": "^5.2.0",
    "phplint": "^2.0.0",
    "sponge-rod": "^3.8.0",
    "underscore": "^1.9.1"
  },
  "dependencies": {}
}

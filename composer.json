{
    "name": "rodrigoiii/framework",
    "description": "Build awesome application using this light but powerful micro framework",
    "type": "project",
    "license": "MIT",
    "keywords": ["micro framework", "framework", "slim", "php"],
    "version": "0.1",
    "authors": [
        {
            "name": "Rodrigo Galura III",
            "email": "rodrigogalura3rd@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "rodrigoiii/framework-core": "^1.0"
    },
    "require-dev": {
        "kint-php/kint": "^2.2",
        "robmorgan/phinx": "^0.9.2",
        "fzaninotto/faker": "^1.7",
        "phpunit/phpunit": "^5.7",
        "runcmf/runtracy": "^0.2.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Framework\\": "framework-core/core/src/psr-4/"
        },
        "files": [
            "framework-core/core/src/settings/functions.php",
            "framework-core/core/src/settings/dotEnv.php",
            "app/helpers.php"
        ]
    },
    "autoload-test": {
        "psr-4": {
            "Framework\\Tests\\": "tests/"
        }
    }
}
